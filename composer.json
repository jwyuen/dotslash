{
  "name": "jwyuen/dotslash",
  "description": "Dotslash: utils for quickly creating command line application in php",
  "type": "library",
  "keywords": [
    "command",
    "command line",
    "command-line",
    "php"
  ],
  "license": "MIT License",
  "authors": [{
    "name": "James Yuen",
    "email": "jwingy@gmail.com",
    "role": "Maintainer"
  }],
  "require": {
    "php": ">=5.3.0",
    "aws/aws-sdk-php": "2.*",
    "monolog/monolog": "1.10.*",
    "jwyuen/configee-php": "v1.2",
    "symfony/console": "2.3.*",
    "symfony/event-dispatcher": "2.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*",
    "mikey179/vfsStream": "v1.2.0"
  },
  "autoload": {
    "classmap": ["src"]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
